if (application.isWorker){

  api.timers.setInterval(function() {
    console.log('Worker Process: ' + application.nodeId);
    console.dir({ workers: application.workers });
    console.log('---');
  }, 5000);

} else {

  api.timers.setInterval(function() {
    console.log('Master Process: ' + application.nodeId);
    console.dir({ workers: application.workers });
    console.log('---');
  }, 5000);

}
